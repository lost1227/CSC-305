{
    "tasks": [
        {
            "type": "process",
            "label": "make",
            "command": "/usr/bin/make",
            "args": [
                "-f",
                "BoardTest.mak"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task to build with make"
        },
        {
            "type": "shell",
            "label": "clean",
            "command": "./clean.py",
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "detail": "Format code with clang-format",
            "problemMatcher": []
        }
    ],
    "version": "2.0.0"
}